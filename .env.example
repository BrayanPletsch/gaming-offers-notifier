# Configurações de e-mail SMTP
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=seu_usuario@gmail.com
SMTP_PASS=sua_senha_app_ou_token

# Destinatário das notificações por e-mail
EMAIL_RECIPIENT=destinatario@example.com

# Número de WhatsApp (inclua DDI e DDD, sem “+” ou zeros à frente)
WHATSAPP_PHONE=55619XXXXXXXXX

# Flags de habilitação: 1 = ativo, 0 = inativo
ENABLE_EMAIL=1
ENABLE_WHATSAPP=1